{
  "name": "server",
  "version": "1.0.0",
  "description": "ReadyForms API server",
  "main": "dist/server.js",
  "scripts": {
    "test": "jest --forceExit",
    "clean": "rimraf dist",
    "build": "rimraf dist && tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "seed": "ts-node src/utils/seed.ts",
    "reset-db": "ts-node src/utils/reset-database.ts",
    "scan-routes": "ts-node src/utils/scan-for-invalid-routes.ts",
    "postinstall": "npm run build",
    "clean-routes": "node src/utils/clean-debug-routes.js",
    "safe-start": "npm run clean-routes && npm run build && npm start",
    "add-columns": "ts-node src/utils/add-missing-columns.ts",
    "fix-db": "npm run add-columns",
    "fix-admin": "ts-node src/utils/fix-admin.ts",
    "prepare-test": "npm run fix-admin && npm run test",
    "debug:scan": "ts-node src/utils/run-debug-scan.ts",
    "debug:templates": "ts-node src/utils/debug-templates.ts",
    "ensure-test-users": "ts-node src/utils/ensure-test-users.ts",
    "test:setup": "npm run ensure-test-users && npm test",
    "pretest": "tsc",
    "test:api": "npm run pretest && jest tests/api.test.js --config=jest.config.js --forceExit --detectOpenHandles --runInBand",
    "test:extended-api": "npm run pretest && jest tests/extended-api.test.js --config=jest.config.js --forceExit --detectOpenHandles --runInBand",
    "test:all": "npm run pretest && jest tests/*.test.js --config=jest.config.js --forceExit --detectOpenHandles --runInBand",
    "vercel-build": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/node": "^22.15.3",
    "@types/uuid": "^10.0.0",
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "path-to-regexp": "^6.2.1",
    "pg": "^8.11.3",
    "pg-connection-string": "^2.6.2",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.35.2",
    "sequelize-typescript": "^2.1.6",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.5",
    "jest": "^29.7.0",
    "rimraf": "^5.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
